{
  "name": "@medanat/lint",
  "version": "3.0.0",
  "description": "Opinionated eslint configuration",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "outdated": "npm outdated && npx lerna exec --no-bail -- npm outdated",
    "lint": "eslint ./",
    "lint-fix": "npm run lint -- --fix",
    "publish": "npx lerna publish -- --access public",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medanat/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "medanat"
  ],
  "author": "Yazan Medanat <medanat+npm@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/medanat/eslint-config/issues"
  },
  "homepage": "https://github.com/medanat/eslint-config#readme",
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.14.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-react": "7.12.4",
    "lerna": "3.13.0"
  },
  "peerDependencies": {
    "eslint": "^5.14.1"
  },
  "engines": {
    "node": ">= 8"
  }
}
